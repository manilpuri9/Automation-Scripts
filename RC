apiVersion: v1
kind: ReplicationController
metadata:
  name: manilpuri9/portfolio:v1
spec:
  replicas: 1
  template:
    metadata:
      labels:
        app: manilpuri9/portfolio:v1
    spec:
      containers:
      - name: manilpuri9/portfolio:v1
        image: manilpuri9/portfolio:v1
        ports:
        - containerPort: 80
